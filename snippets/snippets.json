{
    "VARIABLE LABELS": {
        "prefix": "VARIABLE LABELS",
        "body": "VARIABLE LABELS ${1:variable} \"${2:label}\".$0",
        "description": "VARIABLE LABELS"
    },
    "VARIABLE LEVEL":{
        "prefix": "VARIABLE LEVEL",
        "body": "VARIABLE LEVEL ${1:variable} (${2:scale of measure}).$0",
        "description": "VARIABLE LEVEL"
    },
    "VALUE LABELS":{
        "prefix": "VALUE LABELS",
        "body": ["VALUE LABELS ${1:variable}", 
                 "    ${2:value}.$0"],
        "description": "VALUE LABELS"
    },
    "RECODE":{
        "prefix": "RECODE",
        "body": "RECODE ${1:variable} (${2:values}) ${3:INTO} ${4:new variable}.$0",
        "description": "RECODE"
    },
    "MISSING VALUES":{
        "prefix": "MISSING VALUES",
        "body": "MISSING VALUES ${1:variable} (${2:missing values}).$0"
    },
    "EXECUTE":{
        "prefix": "EXE",
        "body": "EXECUTE."
    },
    "SAVE OUTFILE":{
        "prefix": "SAVE OUTFILE",
        "body": ["SAVE OUTFILE=\"${1:path to file}\"",
                 "    /COMPRESSED."]
    }
    
}